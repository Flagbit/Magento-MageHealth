<?xml version="1.0"?>
<config>
    <modules>
        <Flagbit_MageHealth>
            <version>1.0.0</version>
        </Flagbit_MageHealth>
    </modules>
    <global>
        <models>
            <magehealth>
                <class>Flagbit_MageHealth_Model</class>
            </magehealth>
        </models>
        <helpers>
            <magehealth>
                <class>Flagbit_MageHealth_Helper</class>
            </magehealth>
        </helpers>
    </global>
    <default>
        <magehealth>
            <log>
                <clean_after_day>30</clean_after_day>
                <enabled>0</enabled>
                <time/>
                <frequency>D</frequency>
            </log>
        </magehealth>
    </default>
    <crontab>
        <jobs>
            <magehealth_log_clean>
                <run>
                    <model>magehealth/cron::logClean</model>
                </run>
            </magehealth_log_clean>
        </jobs>
    </crontab>
</config>